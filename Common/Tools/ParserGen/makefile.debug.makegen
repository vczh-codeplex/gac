folder VLPP = ../../../Libraries/GacUI/Public/Source
	h = Vlpp.h
	cpp = Vlpp.cpp

folder PARSERGEN = ./ParserGen/
	h = ParserGen.h
	cpp = ParserGen*.cpp Main.cpp

dependency
	VLPP:cpp < VLPP:h
	PARSERGEN:h < VLPP:h
	PARSERGEN:cpp < PARSERGEN:h

targets
	obj = ./Obj/
	bin = ./Bin/

map cpp : %.cpp
	> o : obj %.o
	g++ -std=c++11 -g -o $(OUT) -c $(IN)

link o :
	> exe : bin ParserGen
	g++ -std=c++11 -g -o $(OUT) $(IN)
