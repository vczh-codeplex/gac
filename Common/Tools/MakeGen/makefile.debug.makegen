folder VLPP = ../../../Libraries/GacUI/Public/Source
	h = Vlpp.h
	cpp = Vlpp.cpp

folder MAKEGEN = .
	h = *.h
	cpp = *.cpp

dependency
	VLPP:cpp < VLPP:h
	MAKEGEN:h < VLPP:h
	MAKEGEN:cpp < MAKEGEN:h

targets
	obj = ./Obj/
	bin = ./Bin/

map cpp : %.cpp
	> o : obj %.o
	g++ -std=c++11 -g -o $(OUT) -c $(IN)

link o :
	> exe : bin MakeGen
	g++ -std=c++11 -g -o $(OUT) $(IN)
