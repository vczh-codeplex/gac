#include
#	../Source/vlpp.makegen

folder VLPP = ../../../Libraries/GacUI/Public/Source
	h = Vlpp.h
	cpp = Vlpp.cpp

folder MAKEGEN = .
	h = *.h
	cpp = *.cpp

dependency
	VLPP:cpp < VLPP:h
	MAKEGEN:h < VLPP:h
	MAKEGEN:cpp < MAKEGEN:h
#	MAKAEGEN:cpp < MAKEGEN:h VLPP:h

map cpp o > ./Bin/*.o
	g++ -std=c++11 -g -o $(OUT) -c $(IN)

pretasks
	mkdir Obj
	mkdir Bin
	
link o exe > ./Bin/MakeGen
	g++ -std=c++11 -g -o $(OUT) $(IN)

